@(Model.displayVar("total_value"))
</span>

			</td>
														<TD class="data @(Model.displayVar("fieldclass_vendnum"))">
				@(Model.displayVar("xvendnum_value"))
				<SPAN data-itemtype="report_grid_field" data-itemid="report_grid_field" @(Model.displayVar("item_report_grid_field")) data-pageid="@(Model.displayVar("pageid"))" data-field="vendnum" data-fieldname="vendnum">
@(Model.displayVar("vendnum_value"))
</span>

			</td>
							</tr>
		}
	
				foreach (string _invoicestatus_endgroup in Model.GetSection("invoicestatus_endgroup", this)){
		<tr class="summaryrow" style="display:none; height:0px;" >
			
										<td  class="group_1" colspan=6 >
				Summary for&nbsp;@(Model.displayVar("label dbo_tp_invoicedetails_Report invoicestatus"))&nbsp;@(Model.displayVar("invoicestatus_grval"))
					- <span class="summary-1" data-type="summary">@(Model.displayVar("groupinvoicestatus_total_cnt"))
					</span> records total			</td>
																																												</tr>

		<!--  Sum -->
		<tr class="grouptotalsrow" style="display:none; height:0px;">
					
						<td class="group_1">Sum</td>
								
						<td class="group_1">&nbsp;</td>
								
						<td class="group_1">&nbsp;</td>
								
						<td class="group_1">&nbsp;</td>
								
						<TD class="@(Model.displayVar("fieldclass_total")) group_1">
				<span class="summary-1" data-type="sum" data-field="total">
					@(Model.displayVar("groupinvoicestatus_totaltotal_sum"))
				</span>
			</td>
								
						<td class="group_1">&nbsp;</td>
							</tr>
	
	
	
	
	}
			
	}


	


    


	</tbody>
</table>


			</div>
}									@foreach (string _below_grid_block in Model.GetSection("below-grid_block", this)){
<DIV class="r-form" data-location="below-grid" id="form_below-grid_@(Model.displayVar("pageid"))" data-makeup="small" @(Model.displayVar("form_below-grid")) data-pageid="@(Model.displayVar("pageid"))">		
	 
