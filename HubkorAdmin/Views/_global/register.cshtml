	</label>
	<div class="r-edit-field">
		@(Model.displayVar("email_editcontrol"))	</div>
	<DIV class="r-edit-tooltip" data-helpfor="email" {$tooltipAttr dbo_users email }>
		<div class="text-muted" >
		@(Model.displayVar("tooltip dbo_users email"))		</div>
	</div>
</div>


}
</td>
							</tr>
					
					</tbody>
				</table>
}
