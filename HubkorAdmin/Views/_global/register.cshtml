	<DIV class="r-edit-tooltip" data-helpfor="confirm" {$tooltipAttr dbo_users confirm_aoel3cx }>
		<div class="text-muted" >
		@(Model.displayVar("tooltip dbo_users confirm_aoel3cx"))		</div>
	</div>
</div>


}
 

